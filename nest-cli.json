{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-demo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-demo/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-demo",
  "projects": {
    "nestjs-demo": {
      "type": "application",
      "root": "apps/nestjs-demo",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-demo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-demo/tsconfig.app.json"
      }
    },
    "logger": {
      "type": "application",
      "root": "apps/logger",
      "entryFile": "main",
      "sourceRoot": "apps/logger/src",
      "compilerOptions": {
        "tsConfigPath": "apps/logger/tsconfig.app.json"
      }
    }
  }
}